{
    "name": "@kequtech/arbor",
    "version": "0.14.1",
    "description": "A minimal, modular Node.js framework for building fast and predictable web applications.",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "sideEffects": false,
    "scripts": {
        "build": "rm -rf dist && tsc -p tsconfig.build.json",
        "prepare": "npm run build",
        "start:basic": "node ./examples/basic/server.ts",
        "start:file-server": "node ./examples/file-server/server.ts",
        "test": "node --test"
    },
    "files": [
        "dist",
        "CHANGELOG.md",
        "LICENSE",
        "README.md"
    ],
    "bugs": {
        "url": "https://github.com/kequtech/arbor/issues"
    },
    "homepage": "https://docs.kequtech.com/arbor/",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/kequtech/arbor.git"
    },
    "author": "Nathan Lunde-Berry",
    "license": "ISC",
    "keywords": [
        "createApp",
        "IncomingMessage",
        "ServerResponse",
        "web",
        "framework",
        "http",
        "rest",
        "server",
        "api"
    ],
    "devDependencies": {
        "@types/node": "^24.0.14",
        "typescript": "^5.9.3"
    }
}
